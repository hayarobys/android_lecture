<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">


	<TextView
		android:id="@+id/textView"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="당신의출생지를 선택하세요."
		android:textSize="20dp"/>

	<Button
		android:id="@+id/btn1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="서울"/>

	<Button
		android:id="@+id/btn2"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="경기도"/>

	<Button
		android:id="@+id/btn3"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="충청도"/>

	<Button
		android:id="@+id/btn4"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="경상도"/>

	<Button
		android:id="@+id/btn5"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="전라도"/>

	<Button
		android:id="@+id/btn6"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="강원도"/>

	<TextView
		android:id="@+id/txtResult"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:textSize="18dp"
		android:textColor="#0e69c4"
		android:text=""/>

</LinearLayout>
